{
  "name": "redux-events-parent",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "rollback": "lerna exec -- npm run rollback",
    "bootstrap": "lerna bootstrap --hoist --force-local --concurrency 1 --no-ci && npm rebuild husky",
    "bootstrap-ci": "lerna bootstrap --hoist --force-local --concurrency 1 --ci",
    "build": "lerna exec --scope redux-simple-events -- npm run build",
    "test": "lerna exec --scope redux-simple-events -- npm run test",
    "test:all": "lerna exec -- npm run test",
    "clean-cache": "npm cache clean --force"
  },
  "dependencies": {
    "lerna": "3.17.0"
  },
  "devDependencies": {
    "cross-spawn": "^7.0.3",
    "eslint": "5.16.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "lint-staged": "10.2.4",
    "prettier": "2.0.5",
    "react-styleguidist": "10.6.1"
  },
  "bundleDependencies": [],
  "keywords": [
    "react",
    "component"
  ],
  "description": "Redux events library",
  "author": "Sergei Malakhov"
}
